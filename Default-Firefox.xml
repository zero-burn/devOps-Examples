<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite guice-stage="DEVELOPMENT" name="Default suite">
	<test verbose="2" name="Default-Firefox">
		<groups>
			<run>
				<exclude name="safari"></exclude>
				<exclude name="chrome"></exclude>
				<exclude name="ie"></exclude>
				<exclude name="opera"></exclude>
			</run>
		</groups>
		<packages>
			<package name="com.sqa.jf.*"></package>
		</packages>
	</test>
</suite>